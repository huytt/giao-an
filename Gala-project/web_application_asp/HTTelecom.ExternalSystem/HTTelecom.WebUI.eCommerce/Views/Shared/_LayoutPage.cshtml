            <div id="main-body">
                @RenderBody()
            </div>
            <footer class="page-footer backgournd-color-main" data-element="background">
                <div class="row user-margin-bottom-0">
                    <div class="col l12 s12">
                        <h5 class="white-text" style="border-bottom: 2px solid white; line-height: 35px;">
                            GALAGALA VIET NAM
                            <a id="footer-gala-title" class="right user-color-white user-margin-right-10" style="font-size:25px;">
                                <i class="fa-icon fa-icon-bottom-114"></i>
                            </a>
                        </h5><div id="footer-article-Galagala" style="display:none;">
                            @Html.Action("Index", "Article", new { lang = ViewBag.currentLanguage })
                        </div>
                        <p class="grey-text text-lighten-4">HOP THANH Trading Business Co.,Ltd</p>
                        <p class="grey-text text-lighten-4">87 Hoang Van Thai, District 7, Ho Chi Minh City, Viet Nam</p>
                    </div>
                </div><div class="footer-copyright"><div class="container">© @DateTime.Now.Year Copyright<a class="grey-text text-lighten-4 right">Galagala Inc.</a></div></div>
            </footer>

            <div class="user-overlay-main" style="opacity: 0; display: none; -webkit-backface-visibility: visible !important;"></div>
            <div id="full" style="position: absolute; top: 0px; left: 0px; z-index: 9999; width: 100%; height: 100%; display: none; background: rgba(0,0,0,0.9);">
                <div style=" height: 40px;">
                    <span style="position: fixed; top: 10px; right: 10px; z-index: 9;" id="btn-normal-screen" data-target="show-full-screen">
                        <img src="~/Images/normal.png" width="20" height="20" />
                    </span>
                </div>
                <div class="content">
                    <div class="view" style="padding:10px;-webkit-box-shadow: 0 2px 5px 0 rgba(0,0,0,.16),0 2px 10px 0 rgba(0,0,0,.12);-moz-box-shadow: 0 2px 5px 0 rgba(0,0,0,.16),0 2px 10px 0 rgba(0,0,0,.12);box-shadow: 0 2px 5px 0 rgba(0,0,0,.16),0 2px 10px 0 rgba(0,0,0,.12);">
                    </div>
                </div>
            </div>
        </div>
