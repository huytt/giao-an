            </div>
            <div class="modal-footer" style="margin-top: 0px;">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">
    var rowCount = 0;
    var imgCount = 0;

    $(".clickChangeMedia").click(function () {
        $($(this).parent().parent().next().children(1).children().children(1)).click();
    });

    function changeImageView(input, number) {
        var img = $(input).attr('data-img');
        var input_file = $(input).attr('data-input');

        if (number == 1) {
            $("#" + img).attr('src', '../../Content/image/icons/icon-NoImage-2.png');
            $("#" + img).attr('data-src', '../../Content/image/icons/icon-NoImage-2.png');
            $('input[name="' + input_file + '"]').val('');
        }
        if (number == 2) {
            $("#" + img).attr('src', '../../Content/image/icons/icon-NoImage-2.png');
            $("#" + img).attr('data-src', '../../Content/image/icons/icon-NoImage-2.png');
            $('input[name="' + input_file + '"]').val('');
        }

        //$('.store-banner-lists').children('tbody').empty();
        rowCount = 0;
        if (window.File && window.FileList && window.FileReader) {
            var files = input.files;
            for (var i = 0; i < files.length; i++) {
                var file = files[i];

                var picReader = new FileReader();
                if (number == 1) {
                    //alert("sdasd");
                    picReader.addEventListener("load", function (event) {
                        var picFile = event.target;
                        $("#" + img).attr('src', picFile.result);
                        $("#" + img).attr('data-src', picFile.result);
                    });
                    $('input[name="' + input_file + '"]').val(file.name);
                }
                if (number == 2) {
                    picReader.addEventListener("load", function (event) {
                        var picFile = event.target;
                        $("#" + img).attr('src', picFile.result);
                        $("#" + img).attr('data-src', picFile.result);
                    });
                    $('input[name="' + input_file + '"]').val(file.name);

                }
                //if (number == 3) {
                //    picReader.onload = (function (file) {
                //        var fileName = file.name;
                //        return function (event) {
                //            var picFile = event.target;

                //            var html = '<tr class="upload-list-' + (rowCount += 1) + '">' +
                //                    '<td>' + rowCount + '</td>' +
                //                    '<td width="50"><img src="' + picFile.result + '" width="40" height="40"/></td>' +
                //                    '<td><strong>' + fileName + '</strong></td>' +
                //                    '<td class="text-right" width="80"></td>' +
                //                    '</tr>'

                //            $('.store-banner-lists').children('tbody').append(html);
                //            $("input[name=storeBInput]").val(rowCount + " Files");

                //        };
                //    })(file);
                //}
                picReader.readAsDataURL(file);
            }
        }
    }

    $('#VendorId').select2().on("select2-selecting", function (e) {
        var _vendorId = e.val;
        if (_vendorId == "") {
            _vendorId = 0;
            $("#vendor-address-table").addClass('hide');
            $("#Vendor_Adddress_Detail").addClass('hide');
        }

        $.ajax({
            type: 'POST',
            url: '@Url.Action("LoadListVendorAddress", "MSS")',
            data: {
                vendorId: _vendorId
            },
            dataType: "json",
            traditional: true,
            success: function (data) {
                if (data.Success) {
                    $("input[name=vendorAddress]").val("");
                    $("input[name=vendorBranch]").val("");
                    $("input[name=vendorCity]").val("");
                    $("input[name=vendorDistrict]").val("");
                    $("input[name=vendorWard]").val("");
                    $("input[name=vendorEmail]").val("");
                    $("input[name=vendorMobile]").val("");
                    $("input[name=vendorTelephone]").val("");
                    $("input[name=vendorGmapLocation]").val("");

                    CreateVendorAddressTable(data._listVendorAddress);
                    if (data._listVendorAddress == "") {
                        $("#vendor-address-table").addClass('hide');
                    }
                    else {
                        $("#vendor-address-table").removeClass('hide');
                    }
                }
            }
        });
    });

    function CreateVendorAddressTable(listVendorAddress) {
        var target = $(".body-vendor-address");
        var _listVendor = JSON.parse('@Html.Raw(Json.Encode(ViewBag.list_Vendor))');
        target.empty();
        $.each(listVendorAddress, function (index, _item) {
            _itemVendorName = "N/A";
            $.each(_listVendor, function (index1, _item1) {
                if (_item1.VendorId == _item.VendorId) {
                    _itemVendorName = _item1.VendorFullName;
                }
            });

            target.append('<tr class="' + (_item.IsDeleted == true ? "danger" : "") + '">'
                            + '<td>' + (index + 1) + '.</td>'
                            + '<td class="text-center">'
                                + (_item.IsActive == true
                                    ? '<span class="label label-success" data-toggle="tooltip" data-placement="top" data-original-title="Active">Active</span>'
                                    : '<span class="label label-danger" data-toggle="tooltip" data-placement="top" data-original-title="Inactive">Inactive</span>')
                            + '</td>'
                            + '<td>' + _itemVendorName + '</td>'
                            + '<td>' + _item.Address + '</td>'
                            + '<td>'
                                + '<button type="button" class="btn btn-grey mb5" onclick="VendorAddressDetail(' + _item.VendorAddressId + ')">Detail</button>'
                            + '</td>'
                        + '</tr>')
        });
    }
    function VendorAddressDetail(vendorAddressId) {
        //alert(vendorAddressId);
        $.ajax({
            type: 'POST',
            url: '@Url.Action("LoadVendorAddress", "MSS")',
            data: {
                vendorAddressId: vendorAddressId
            },
            dataType: "json",
            traditional: true,
            success: function (data) {
                item = data._vendorAddress;
                if (data.Success) {
                    $("input[name=vendorAddress]").val(item.Address);
                    $("input[name=vendorBranch]").val(item.LocationName);
                    $("input[name=vendorCity]").val(item.City);
                    $("input[name=vendorDistrict]").val(item.District);
                    $("input[name=vendorWard]").val(item.Ward);
                    $("input[name=vendorEmail]").val(item.Email);
                    $("input[name=vendorMobile]").val(item.Mobilephone);
                    $("input[name=vendorTelephone]").val(item.Telephone);
                    $("input[name=vendorGmapLocation]").val(item.GGMapLink);
                    $("#Vendor_Adddress_Detail").removeClass('hide');
                }
            }
        });
    }

    function previewProduct(id) {
        var iframe = document.getElementById("framePreview");
        iframe.src = 'http://galagala.vn:/Product/PreviewProduct/' + id;
        $("#modalPreview").modal('show');
    }

    // checked Product table
    // ================================
    function ChangeStatusAction(type, productId, sbool) {
        _listCheckedProductTable = [];

        if (productId == 0) {
            //alert(productId);
            $('.list-product-table tr').each(function () {
                var achecked = $(this).find('input[name="chk_product-table"]');
                if (achecked.is(':checked')) {
                    _listCheckedProductTable.push(achecked.val());
                }
            });
        }
        else {
            //alert(productId);
            _listCheckedProductTable.push(productId);
        }

        if (type == 1) { //Verified action
            //alert(sbool)
            if (confirm("Are you sure?")) {
                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("ChangeProductVerified", "MSS")',
                    data: {
                        storeId: '@Model.StoreId',
                        listProductId: _listCheckedProductTable,
                        sbool: sbool,
                        page: '@ViewBag.page'
                    },
                    dataType: "json",
                    traditional: true,
                    success: function (data) {
                        if (data.Success) {
                            createProductTable(data._listProduct);
                        }
                    }
                });
            }
        }
        if (type == 2) { //Active action

            if (confirm("Are you sure?")) {
                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("ChangeProductActive", "MSS")',
                    data: {
                        storeId: '@Model.StoreId',
                        listProductId: _listCheckedProductTable,
                        sbool: sbool,
                        page: '@ViewBag.page'
                    },
                    dataType: "json",
                    traditional: true,
                    success: function (data) {
                        if (data.Success) {
                            createProductTable(data._listProduct);
                        }
                    }
                });
            }
        }
    }
    function createProductTable(listProductTable) {
        var target = $(".list-product-table");
        var _listProductType = JSON.parse('@Html.Raw(Json.Encode(ViewBag.list_ProductType))');
        target.empty();
        $.each(listProductTable, function (_index, _item) {

            var itemProductTypeName = "";
            var url = '@Url.Action("ProductEdit", "MSS", new { id = "_ProductId_" })'.replace('_ProductId_', _item.ProductId);
            var dateCreated = new Date(_item.DateCreated.match(/\d+/)[0] * 1);

            $.each(_listProductType, function (_index, _item1) {
                if (_item1.ProductTypeCode == _item.ProductTypeCode) {
                    itemProductTypeName = _item1.ProductTypeName;
                }
            });

            target.append(
                '<tr class="' + (_item.IsDeleted == true ? "bg-danger" : "") + '">'
                    + '<td class="text-center">'
                        + '<div class="checkbox custom-checkbox nm">'
                            + '<input type="checkbox" name="chk_product-table" id="chk_product-table-' + _index + '" value="' + _item.ProductId + '" data-toggle="selectrow" data-target="tr" data-contextual="' + (_item.IsDeleted == true ? "danger" : "info") + '"/>'
                            + '<label for="chk_product-table-' + _index + '"></label>'
                        + '</div>'
                    + '</td>'
                    + '<td><span style="font-size:12px">' + _item.ProductStockCode + '</span></td>'
                    + '<td class="text-center">'
                        + (_item.IsActive == true
                            ? '<a href="javascript:void(0);" onclick="ChangeStatusAction(2,' + _item.ProductId + ',-1);"><span class="label label-success" data-toggle="tooltip" data-placement="top" data-original-title="Active">Active</span></a>'
                            : '<a href="javascript:void(0);" onclick="ChangeStatusAction(2,' + _item.ProductId + ',-1);"><span class="label label-danger" data-toggle="tooltip" data-placement="top" data-original-title="Inactive">Inactive</span></a>')
                    + '</td>'
                    + '<td class="text-center">'
                        + (_item.IsVerified == true
                            ? '<a href="javascript:void(0);" onclick="ChangeStatusAction(1,' + _item.ProductId + ',-1);"><span class="label label-success" data-toggle="tooltip" data-placement="top" data-original-title="Verified">Verified</span></a>'
                            : '<a href="javascript:void(0);" onclick="ChangeStatusAction(1,' + _item.ProductId + ',-1);"><span class="label label-danger" data-toggle="tooltip" data-placement="top" data-original-title="Unverified">Unverified</span></a>')
                    + '</td>'
                    + '<td><a href="' + url + '"><strong>' + _item.ProductName + '</strong></a></td>'
                    + '<td class="text-center">'
                        + '<a href="#" onclick="previewProduct(' + _item.ProductId + ')"><i class="fa fa-2x fa-video-camera text-primary"></i></a>'
                    + '</td>'
                    + '<td>' + itemProductTypeName + '</td>'
                    + '<td>' + moment(dateCreated).format('MM/DD/YYYY') + '</td>'
                + '</tr>'
                            );
        });
    }


    $(function () {
        // Ladda button
        // ================================
        Ladda.bind('.btn.ladda-button', { timeout: 50000 });

        // Bind progress buttons and simulate loading progress
        Ladda.bind('.btn.ladda-button.ladda-progress', {
            callback: function (instance) {
                var progress = 0;
                var interval = setInterval(function () {
                    progress = Math.min(progress + Math.random() * 0.1, 1);
                    instance.setProgress(progress);

                    if (progress === 1) {
                        instance.stop();
                        clearInterval(interval);
                    }
                }, 200);
            }
        });
        // Lightbox
        // ================================
        $('#shuffle-SHB').magnificPopup({
            delegate: '.magnific',
            type: 'image',
            gallery: {
                enabled: true
            }
        });
        $('#shuffle-SB').magnificPopup({
            delegate: '.magnific',
            type: 'image',
            gallery: {
                enabled: true
            }
        });
        $('#shuffle-SCNB').magnificPopup({
            delegate: '.magnific',
            type: 'image',
            gallery: {
                enabled: true
            }
        });
        // ================================


        $("#Keywords").selectize({
            plugins: ['remove_button'],
            delimiter: ',',
            persist: false,
            create: function (input) {
                return {
                    value: input,
                    text: input
                }
            },
            render: {
                item: function (data, escape) {
                    return '<div>' + escape(data.text) + '</div>';
                }
            },
            //onDelete: function (values) {
            //    return confirm(values.length > 1 ? 'Are you sure you want to remove these ' + values.length + ' items?' : 'Are you sure you want to remove "' + values[0] + '"?');
            //}
        });

        $('.date').datepicker({
            changeMonth: true,
            changeYear: true
        });
        $('#VendorId').select2();

        $("img[data-toggle~='unveil1']").unveil(200, function () {
            $(this).load(function () {
                $(this).addClass('unveiled');
            });
        });

        // Sparkline
        // ================================
        $('.sparklines').sparkline('html', {
            enableTagOptions: true
        });

        // globar variable within this function scope
        var panel = '.panel#product-table',
            count = 0;

        // subscribe to `selected` row event
        $(document).on('fa.selectrow.selected', function (event, data) {
            // define variable
            var $panel = data.element.parents(panel),
                $toolbar = $panel.find('#toolbar-toshow');

            // check for the count panel #toolbar-showcase only
            // as we dont want the event affect other panel/table
            if ($panel.length > 0) {
                // total the count variable
                count = count + 1;

                // check for the count panel #toolbar-showcase only
                if (count === 1) {
                    $toolbar.removeClass('hide');
                }
            }
        });

        // subscribe to `unselected` row event
        $(document).on('fa.selectrow.unselected', function (event, data) {
            // define variable
            var $panel = data.element.parents(panel),
                $toolbar = $panel.find('#toolbar-toshow');

            // check for the count panel #toolbar-showcase only
            // as we dont want the event affect other panel/table
            if ($panel.length > 0) {
                // minus the count variable
                count = count - 1;

                // check for the count
                if (count === 0) {
                    $toolbar.addClass('hide');
                }
            }
        });
    });
</script>
