                                                <th>Address</th>
                                                <th width="7%"> Handling</th>
                                            </tr>
                                        </thead>
                                        <tbody class="body-vendor-address">

                                            @foreach (var item in (List<VendorAddress>)ViewBag.list_VendorAddress)
                                                    {
                                                        var itemVendor = ((List<Vendor>)ViewBag.list_Vendor).Where(a => a.VendorId == item.VendorId).FirstOrDefault();
                                                    <tr class="@(item.IsDeleted == true ? "danger" : "")">
                                                        <td>@i</td>
