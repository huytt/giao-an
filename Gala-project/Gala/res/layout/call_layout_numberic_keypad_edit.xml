            layout="@layout/call_layout_numberic_keypad_detail" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_weight="2"
        android:weightSum="6" >

        <include
            android:id="@+id/view_keypad_buttons_star"
            android:layout_width="fill_parent"
