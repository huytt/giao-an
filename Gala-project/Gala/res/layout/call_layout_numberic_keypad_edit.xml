            android:id="@+id/view_keypad_buttons_1"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_margin="2dp"
            android:layout_weight="2"
            layout="@layout/call_layout_numberic_keypad_detail" />

