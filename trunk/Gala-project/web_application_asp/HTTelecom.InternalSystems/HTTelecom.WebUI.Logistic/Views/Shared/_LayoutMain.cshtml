            setInterval(getNewOrder, 5000);
        });
        function getNewOrder() {
            $.ajax({
                type: "POST",
                url: '@Url.Action("GetNotification", "TTS")',
                data: {},
                cache: false,
                dataType: "json",
                success: function (dt) {
                    var nOrder = false;
                    var t = $("#toaster_order");
                    var lst = getCookie('lstOrder'); lst = lst == "" ? [] : JSON.parse(lst);
                    var dNow = new Date();
                    for (var i = 0; i < dt.data.length; i++) {

                        if (t.find('li[data-value="' + dt.data[i].MainRecordId + '"]').length == 0) {
                            var dd = new Date(parseInt(dt.data[i].DateModified.substr(6).replace('/', '').replace(')', '')));
