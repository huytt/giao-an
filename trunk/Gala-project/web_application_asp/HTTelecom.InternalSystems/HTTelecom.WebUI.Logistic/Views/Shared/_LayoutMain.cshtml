                            href = href.replace("_id_", dt.data[i].MainRecordId),
                            t.append('<li class="itemOrder" data-value="' + dt.data[i].MainRecordId + '"><a href="' + href + '">' + strDate + " Order Id: "+dt.data[i].FormId + '</a></li>');
                            lst.push({ d: dt.data[i].DateModified, m: dt.data[i].MainRecordId, f: dt.data[i].FormId });
                            nOrder = true;
                        }
                    }
                    if (nOrder == true)
                        document.getElementById('audio_chat').play();
                    setCookie('lstOrder', JSON.stringify(lst), 364), $("#countOrderLayout").text(t.find(".itemOrder").length.toString());
                }, error: function (error) { }
            }); //end ajax call
        }
        </text>
        }
        function getCookie(cname) {
            var dt = localStorage.getItem(cname), dt = dt == null ? "" : dt; return dt;
        } function setCookie(cname, cvalue, exdays) {
            localStorage.setItem(cname, cvalue)
        }
        function clone(obj) {
            var copy;

            // Handle the 3 simple types, and null or undefined
            if (null == obj || "object" != typeof obj) return obj;

            // Handle Date
            if (obj instanceof Date) {
                copy = new Date();
                copy.setTime(obj.getTime());
                return copy;
            }

            // Handle Array
            if (obj instanceof Array) {
                copy = [];
                for (var i = 0, len = obj.length; i < len; i++) {
                    copy[i] = clone(obj[i]);
                }
                return copy;
            }

            // Handle Object
            if (obj instanceof Object) {
                copy = {};
                for (var attr in obj) {
                    if (obj.hasOwnProperty(attr)) copy[attr] = clone(obj[attr]);
                }
                return copy;
            }

            throw new Error("Unable to copy obj! Its type isn't supported.");
        }
