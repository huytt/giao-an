@{
    ViewBag.Action = "storeEditForm";
    ViewBag.Controller = "MSS";
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
    int j = 1;
    int i = 1;
    int k = 1;
    var ddl_Vendor = ((List<Vendor>)ViewBag.list_Vendor).Where(a => a.IsActive == true && a.IsDeleted == false).ToList();
    List<string> listWarning = (List<string>)TempData["listStoreWarning"];

    ViewBag.TabIsActive = "tabone";
    if (TempData["tabActive"] != null)
    {
        ViewBag.TabIsActive = TempData["tabActive"];
    }
}
@using PagedList;
@using PagedList.Mvc;
@using HTTelecom.Domain.Core.DataContext.mss
@using HTTelecom.Domain.Core.DataContext.cis
@using HTTelecom.WebUI.MediaSupport.Common

@model Store

<!-- Page Header -->
<div class="page-header page-header-block">
    <div class="page-header-section">
        <h4 class="title semibold">Store - @String.Format("S{0}", @Model.StoreCode)</h4>
    </div>
    <div class="page-header-section">
        <!-- Toolbar -->
        <div class="toolbar">
            <ol class="breadcrumb breadcrumb-transparent nm">
                <li><i class="fa fa-home"></i>&nbsp;<a href="@Url.Action("Index","Home")">Home</a></li>
                <li><i class="fa fa-shopping-cart"></i>&nbsp;<a href="@Url.Action("StoreIndex","MSS")">Store</a></li>
                <li class="active">Edit Store</li>
            </ol>

        </div>
        <!--/ Toolbar -->
    </div>
</div>

@if (ViewBag.SuccessAlert == "SuccessAlert")
{
    <div class="alert alert-success">
        <a href="#" class="close" data-dismiss="alert">&times;</a>
        <span><strong>Saved @String.Format("S{0}", Model.StoreCode) successful!</strong> Your operation has been done successfully !!!</span>
    </div>
}
@if (ViewData.ModelState.Where(a => a.Value.Errors.Count > 0).ToList().Count > 0)
{
    if ((int)TempData["StatusMessage"] == 0)
    {
        <div class="alert alert-danger">
            <a href="#" class="close" data-dismiss="alert">&times;</a>
            <span class="semibold">@Html.ValidationSummary(false)</span>
        </div>
    }
    if ((int)TempData["StatusMessage"] == 1)
    {
        <div class="alert alert-warning">
            <a href="#" class="close" data-dismiss="alert">&times;</a>
            <span><strong>WARNING @String.Format("S{0}", Model.StoreCode)!</strong> Condition to display Store online:</span>
            <ul>
                @foreach (var item in listWarning)
                {
                    <li>@item.ToString()</li>
                }
            </ul>
        </div>
    }
}

<div class="row">
    <!--/ Page Header -->
    @using (Html.BeginForm("StoreEdit", "MSS", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        @Html.HiddenFor(model => model.StoreId)
