            {
                <Text>lstProduct.push({ ProductId: '@item.Item1', Quantity: '@item.Item2' });</Text>
            }
            var jJson = { lstProduct: lstProduct };
            $.ajax({
                type: "GET",
                url: '@Url.Action("GetShippingFee","Particle")',
