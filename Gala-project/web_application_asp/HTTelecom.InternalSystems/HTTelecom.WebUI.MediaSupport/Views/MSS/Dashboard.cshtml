                            <div class="indicator show"><span class="spinner"></span></div>
                            <!--/ Loading indicator -->
                            <div class="chart mt10" id="chart-store" style="height:250px;"></div>
                        </div>
                    </div>
                    <!--/ panel body with collapse capabale -->
