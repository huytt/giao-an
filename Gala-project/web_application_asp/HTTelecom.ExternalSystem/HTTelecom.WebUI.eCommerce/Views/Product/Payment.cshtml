        </h4>
        <div class="col s12 m12" style="MIN-HEIGHT: 100px;">
            <table>
                <thead>
                    <tr>
                        <th>#</th>
                        <th>@((string)ViewBag.multiRes.GetString("name", ViewBag.CultureInfo))</th>
                        <th>@((string)ViewBag.multiRes.GetString("price", ViewBag.CultureInfo))</th>
                        <th>@((string)ViewBag.multiRes.GetString("quantity", ViewBag.CultureInfo))</th>
                        <th>@((string)ViewBag.multiRes.GetString("total", ViewBag.CultureInfo))</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in lst)
                    {
                        var itemProduct = lstProduct.Where(n => n.ProductId == item.Item1).FirstOrDefault();
                        var hrefProduct = itemProduct.ProductTypeCode == "PT1" ? "OrdProduct" : itemProduct.ProductTypeCode == "PT2" ? "SaleProduct" : itemProduct.ProductTypeCode == "PT3" ? "ChargeProduct" : "FreeProduct";
                        var price = itemProduct.PromotePrice != null && itemProduct.PromotePrice < itemProduct.MobileOnlinePrice ? itemProduct.PromotePrice : itemProduct.MobileOnlinePrice;
                        var quantity = lst.Where(n => n.Item1 == itemProduct.ProductId).FirstOrDefault().Item2;
                        //var Size = lst.Where(n => n.Item1 == itemProduct.ProductId && n).FirstOrDefault().Item3;
                        var size = ListSize.Where(n => n.SizeId == item.Item3).FirstOrDefault();
                        <tr>
                            <td>@index</td>
                            <td colspan="4">
                                <a href="@Url.Action(hrefProduct, "Product", new { id = itemProduct.ProductId, urlName = itemProduct.Alias, urlNameStore = itemProduct.Store.Alias })">
                                    @itemProduct.ProductName @(size != null && size.Code != "OneSize" ? "(" + size.SizeName + ")" : "")
                                </a>
                            </td>
                        </tr>
                        <tr>
                            <td colspan="2"></td>
                            <td>@string.Format("{0:0,0 đ}", price)</td>
                            <td>@quantity</td>
                            <td class="text-right">@string.Format("{0:0,0 đ}", price * quantity)</td>
                        </tr>
                        total += Convert.ToDouble(price * quantity);
                        index++;
                    }
                </tbody>
                <tfoot>
                    @{
                    var shippingfee = ship == null || Convert.ToDecimal(total) >= ship.FreeShip ? 0 : ViewBag.Shippingfee;
                    var transactionfee = ship == null || Convert.ToDecimal(total) >= ship.FreeShip ? 0 : ship.Price;
                    }
                    <tr>
                        <th colspan="4">@((string)ViewBag.multiRes.GetString("subtotalfee", ViewBag.CultureInfo))</th>
                        <th class="text-right" data-money="@total">@string.Format("{0:0,0 đ}", total)</th>
                    </tr>
                    @*<tr>
                            <th colspan="4">@((string)ViewBag.multiRes.GetString("taxfee", ViewBag.CultureInfo))</th>
                            <th class="text-right">@string.Format("{0:0,0 %}", 0)</th>
                        </tr>*@
                    <tr>
                        <th colspan="4">@((string)ViewBag.multiRes.GetString("shippingfee", ViewBag.CultureInfo))</th>
                        <th class="text-right" id="shippingfee" data-money="@shippingfee">
                            @string.Format("{0:0,0 đ}", shippingfee)
                        </th>
                    </tr>
                    <tr>
                        <th colspan="4">@((string)ViewBag.multiRes.GetString("total_price", ViewBag.CultureInfo))</th>
                        <th class="text-right" id="total_price">@string.Format("{0:0,0 đ}", Convert.ToDecimal(total) + shippingfee + transactionfee)</th>
                    </tr>
                </tfoot>
            </table>
        </div>
    </div>
    <div class="row user-margin-10 backgournd-color-white user-border-radius-5" style="box-shadow: 0px 1px 1px 0px #c3c3c3;">
