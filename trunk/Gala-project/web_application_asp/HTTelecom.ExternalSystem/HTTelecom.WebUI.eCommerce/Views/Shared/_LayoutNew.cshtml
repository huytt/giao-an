<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.LayoutTitle</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no,minimum-scale=1" />
    <meta name="keywords" content="@ViewBag.LayoutKeywords">
    <meta name="robots" content="index, follow">
    <meta name="author" content="Team Galagala.vn Coryright 2015">
    <meta name="description" content="@ViewBag.LayoutDescription">
    <meta name="google-site-verification" content="">
    <meta property="og:title" content="@ViewBag.Layout_ogTitle">
    <meta property="og:type" content="mobile">
    <meta property="og:image" content="@ViewBag.Layout_image">
    <meta property="og:description" content="@ViewBag.Layout_description">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-title" content="GALAGALA.VN">
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="~/Content/icon.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="~/Content/icon.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="~/Content/icon.png">
    <link rel="apple-touch-icon-precomposed" href="~/Content/icon.png">
    <link rel="shortcut icon" href="~/Content/icon.png">
    <link rel="icon" href="~/Content/icon.png" type="image/x-icon">
    <link rel="shortcut icon" href="~/Content/icon.png" type="image/x-icon">
    <meta name="apple-mobile-web-app-capable" content="yes">
    @Styles.Render("~/www.galagala.vn/css/vs-mb")
    @Styles.Render("~/www.galagala.vn/css/vs-mb/all")
    @RenderSection("CSS", required: false)
    <script>window.lazySizesConfig = window.lazySizesConfig || {}; window.lazySizesConfig.expand = 10;</script>
</head>
<body>
    <div id="main-container-user">
        <aside class="menu-left" style="z-index:0">
            <nav id="side-nav-left" class="side-nav">
                <div class="target">
                    <div id="side-nav-left-content">
                        <ul class="mLeft" data-number="1" data-level="0">
                            @{
                                var Account = (HTTelecom.Domain.Core.DataContext.cis.Customer)Session["sessionGala"];
                                var Vendor = (HTTelecom.Domain.Core.DataContext.cis.Vendor)Session["sessionVendorGala"];
                            }
                            <li>
                                <a href="/">
                                    <i class="fa-menu fa-menu-home-13"></i>
                                    @((string)ViewBag.multiRes.GetString("Home", ViewBag.CultureInfo))
                                </a>
                            </li>
                            <li>
                                <a class="children-item cursor-point" data-show="2" data-target="#side-nav-left-content" data-valuechange="-280px" data-level="2" data-menushow="category">
                                    <i class="fa-menu fa-menu-category-5"></i>
                                    @((string)ViewBag.multiRes.GetString("categories", ViewBag.CultureInfo))
                                    <span class="secondary-content Cate_ProCount" style="padding:0px;">
                                        <i class="fa-icon fa-icon-right-44" style="float: right; transform: scale(0.35); -ms-transform: scale(.35); -webkit-transform: scale(.35); -moz-transform: scale(.35); -o-transform: scale(.35); -webkit-transform: scale(.35); margin-top: 6px; "></i>
                                    </span>
                                </a>
                            </li>
                            <li>
                                <a href="@Url.Action("All","Brand")">
                                    <i class="fa-menu fa-menu-brand-7"></i>
                                    @((string)ViewBag.multiRes.GetString("brand", ViewBag.CultureInfo))
                                </a>
                            </li>
                            <li>
                                <a href="@Url.Action("Wishlist","Product")">
                                    <i class="fa-menu fa-menu-heart-10"></i>
                                    @((string)ViewBag.multiRes.GetString("wishlist", ViewBag.CultureInfo))
                                </a>
                            </li>
                            <li>
                                <a href="@Url.Action("Profile", "Customer")">
                                    <i class="fa-menu fa-menu-account-12"></i>
                                    @((string)ViewBag.multiRes.GetString("profile", ViewBag.CultureInfo))
                                </a>
                            </li>
                            @if (Vendor != null)
                            {
                            <li>
                                <a href="@Url.Action("index", "Vendor")">
                                    <i class="fa-menu fa-menu-account-12"></i>
                                    @((string)ViewBag.multiRes.GetString("page", ViewBag.CultureInfo)) Vendor
                                </a>
                            </li>
                            }
                            @if (Account == null)
                            {
                            <li>
                                <a href="@Url.Action("Login","Customer")">
                                    <i class="fa-menu fa-menu-sign-in-8"></i>
                                    @((string)ViewBag.multiRes.GetString("login", ViewBag.CultureInfo))
                                </a>
                            </li>
                            <li>
                                <a href="@Url.Action("SignUp", "Customer")">
                                    <i class="fa-menu  fa-menu-sign-out-2"></i>
                                    @((string)ViewBag.multiRes.GetString("sign_up", ViewBag.CultureInfo))
                                </a>
                            </li>
                            }

                            <li>
                                <a>
                                    <i class="fa-menu fa-menu-help-3"></i>
                                    @((string)ViewBag.multiRes.GetString("help", ViewBag.CultureInfo))
                                </a>
                            </li>
                            <li>
                                <a class="children-item cursor-point" data-show="2" data-target="#side-nav-left-content" data-valuechange="-280px" data-level="2" data-menushow="language">
                                    <i class="fa-menu fa-menu-global-4"></i>
                                    @((string)ViewBag.multiRes.GetString("Langague", ViewBag.CultureInfo))
                                    <span class="secondary-content Cate_ProCount" style="padding:0px;">
                                        <i class="fa-icon fa-icon-right-44" style="float:right; transform:  scale(0.35); -ms-transform: scale(.35); -webkit-transform: scale(.35); -moz-transform: scale(.35); -o-transform: scale(.35); -webkit-transform: scale(.35); margin-top: 6px; "></i>
                                    </span>
                                </a>
                            </li>
                            <li>
                                <a href="@Url.Action("Info", "Article", new { id = 42 })">
                                    <i class="fa-menu fa-menu-sign-out-2"></i>
                                    @((string)ViewBag.multiRes.GetString("terms_of_usage", ViewBag.CultureInfo))
                                </a>
                            </li>
                            <li>
                                <a href="@Url.Action("Info", "Article", new { id = 26 })">
                                    <i class="fa-menu fa-menu-key-9"></i>
                                    @((string)ViewBag.multiRes.GetString("private_policy", ViewBag.CultureInfo))
                                </a>
                            </li>

                            @if (Account != null)
                            {
                            <li>
                                <a href="@Url.Action("Logout", "Customer")">
                                    <i class="fa-menu fa-menu-sign-out-2"></i>
                                    @((string)ViewBag.multiRes.GetString("logout", ViewBag.CultureInfo))
                                </a>
                            </li>
                            }
                        </ul>
                        <ul class="mLeft" data-level="2" data-content="language">
                            <li style="background-color: #e9e9e9; text-align: left; line-height: 42px; height: 42px; padding:0px;color:black;">
                                <a class="mLeft-move cursor-point user-col-0 user-padding-0" data-value="0px" data-show="0" data-target="#side-nav-left-content" style=" float: left;padding: 0px 10px;">
                                    <i class="fa-icon fa-icon-left-81" style="float: left; transform: scale(0.35); -ms-transform: scale(.35); -webkit-transform: scale(.35);-moz-transform: scale(.35);-o-transform: scale(.35); -webkit-transform: scale(.35); margin-top: 6px; "></i>
                                    @((string)ViewBag.multiRes.GetString("Langague", ViewBag.CultureInfo))
                                </a>
                            </li>
                            <li>
                                @{
                                    var sessionObject = (HTTelecom.WebUI.eCommerce.Common.SessionObject)Session["sessionObject"];
                                }
                                <a href="@(sessionObject == null || sessionObject.lang == "vi" ? "#" : Url.Action("setLanguage", "Home", new { lang = "Vietnamese", u = ViewBag.u }))"
                                   style="@(sessionObject == null || sessionObject.lang == "vi" ? "background-color: #c3c3c3;" : "")">
                                    <span class="secondary-content Cate_ProCount cate-image">
                                        <i class="fa-icon fa-icon-vietname-92" style="float: left;"></i>
                                    </span>
                                    Vietnamese
                                    @if (sessionObject == null || sessionObject.lang == "vi")
                                    {
                                    <span class="secondary-content Cate_ProCount" style="padding:0px;">
                                        <i class="fa-icon fa-icon-check-130" style="float: right;"></i>
                                    </span>
                                    }
                                </a>
                            </li>
                            <li>
                                <a href="@(sessionObject != null && sessionObject.lang == "en" ? "#" : Url.Action("setLanguage", "Home", new { lang = "English", u = ViewBag.u }))"
                                   style="@(sessionObject != null && sessionObject.lang == "en" ? "background-color: #c3c3c3;" : "")">
                                    <span class="secondary-content Cate_ProCount cate-image">
                                        <i class="fa-icon fa-icon-english-11" style="float: left;"></i>
                                    </span>
                                    English
                                    @if (sessionObject != null && sessionObject.lang == "en")
                                    {
                                    <span class="secondary-content Cate_ProCount" style="padding:0px;">
                                        <i class="fa-icon fa-icon-check-130" style="float: right;"></i>
                                    </span>
                                    }
                                </a>
                            </li>
                            <li>
                                <a href="@(sessionObject != null && sessionObject.lang == "zh" ? "#" : Url.Action("setLanguage", "Home", new { lang = "Chinese", u = ViewBag.u }))"
                                   style="@(sessionObject != null && sessionObject.lang == "zh" ? "background-color: #c3c3c3;" : "")">
                                    <span class="secondary-content Cate_ProCount cate-image" style="padding: 0px 10px; float: left;">
                                        <img src="~/Images/China.png" alt="China" style="width: 13.2px; height: 13.2px; margin-right: 5px; display: inline-block; font-size: inherit; text-rendering: auto; -webkit-font-smoothing: antialiased; -moz-osx-font-smoothing: grayscale;" />
                                    </span>
                                    Chinese
                                    @if (sessionObject != null && sessionObject.lang == "zh")
                                    {
                                    <span class="secondary-content Cate_ProCount" style="padding:0px;">
                                        <i class="fa-icon fa-icon-check-130" style="float: right; transform: scale(0.35); -ms-transform: scale(.35); -webkit-transform: scale(.35); -moz-transform: scale(.35); -o-transform: scale(.35); -webkit-transform: scale(.35); margin-top: 6px; "></i>
                                    </span>
                                    }
                                </a>
                            </li>
                        </ul>
                        <div id="content-Category">
                            <ul id="lstCategory1" class="mLeft" data-level="2" data-content="category"
                                style="margin: 0px; margin-left: 280px; padding-top: 0px; display: none; ">
                                <li style="text-align:center;">
                                    <img class="loader" src="~/Images/ajax-loader-fb.gif" />
                                </li>
                            </ul>
                            <ul id="lstCategory2" class="mLeft" data-level="3"
                                style="margin: 0px; left: 600px; padding-top: 0px; display: none;">
                                <li style="text-align:center;">
                                    <img class="loader" src="~/Images/ajax-loader-fb.gif" />
                                </li>
                            </ul>
                            <ul id="lstCategory3" class="mLeft" data-level="4"
                                style=" margin: 0px;  left: 900px;padding-top: 0px; display: none;">
                                <li style="text-align:center;">
                                    <img class="loader" src="~/Images/ajax-loader-fb.gif" />
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </nav>
            <div id="displayCart">
                <div style="position:relative;">
                    <img src="/Images/bag.png" width="34" height="34">
                    <p style="margin: 0px;line-height: 35px;height: 27px;font-size: 16px;">@((string)ViewBag.multiRes.GetString("cart", ViewBag.CultureInfo))</p>
                    <a id="menuLeft-moneyCart">
                        @ViewBag.totalMoneyCart
                    </a>
                    <p style=" margin: 0px; line-height: 24px; height: 24px; font-size: 12px;"> Đã bao gồm thuế VAT</p>
                </div>
            </div>
        </aside>
        <div class="inner-wrap">
            <!-- Nav MenuTOp & Left-->
            <nav id="navTop" class="backgournd-color-main">
                <div class="nav-wrapper backgournd-color-main" id="header-menu">
                    <ul class="right user-padding-right-10" id="user-header-content">
                        <li class="user-reponsive-relative">
                            <a href="@Url.Action("Wishlist","product")">
                                <i class="fa-icon fa-icon-heart-23" style="font-size: 22px;"></i>
                                @Html.Raw("<span class='user-header-count' id='header-count-wishlist'>" + ViewBag.Layout_CountWishList + "</span>")
                            </a>
                        </li>
                        <li class="user-reponsive-relative">
                            <a href="@Url.Action("Cart", "Product" )">
                                <i class="fa-icon fa-icon-cart-21"></i>
                                @Html.Raw("<span class='user-header-count' id='header-count-cart'>" + ViewBag.Layout_CountCart + "</span>")
                            </a>
                        </li>
                    </ul>
                    <ul class="left" id="user-header-menu">
                        <li id="btnbacktoback"><a href="@ViewBag.Back"><i class="fa-icon fa-icon-left-82"></i></a></li>
                        <li>
                            <a id="menu-btnMenuLeftNew" data-activates="side-nav-left" class="button-collapse">
                                <i class="fa-icon fa-icon-menu-66"></i>
                            </a>
                        </li>
                        <li><a id="header-btnSearch" class="cursor-point"><i class="fa-icon fa-icon-search-52"></i></a></li>
                    </ul>
                    <a href="/">
                        <img src="/Images/logo_web.png" id="logoGalagala">
                    </a>
                </div>
            </nav>
            <!-- Nav Search-->
            <nav id="navTop-Search" class="backgournd-color-main">
                <div class="nav-wrapper">
                    <a id="btn-close-Search">
                        <span class="fa-icon fa-icon-left-82"></span>
                    </a>
                    <input id="Seach-txtSearch" class="txtSearch" name="Seach-txtSearch" type="search"
                           placeholder="@((string)ViewBag.multiRes.GetString("input_product", ViewBag.CultureInfo))"
                           required="" data-url="@Url.Action("search_index", "Particle")" />
                    <a id="Seach-btnSearch" class="btnSearch" data-href="@Url.Action("Index", "Search" , new {q="__q__" })">
                        @*<span class="fa-icon fa-icon-search-51"></span>*@
                        @((string)ViewBag.multiRes.GetString("search", ViewBag.CultureInfo))
                    </a>
                </div>
            </nav>
            <section class="main-section" style="padding-top: 42px;min-height:500px;">
                @RenderBody()
                <div id="clear"></div>
            </section>
            <footer class="page-footer">
                <div class="row user-margin-bottom-0">
                    <div class="col l12 s12">
                        <h5 class="white-text" style="border-bottom: 2px solid white; line-height: 35px;">
                            GALAGALA VIET NAM
                            <a id="footer-gala-title" class="right user-color-white user-margin-right-10">
                                <i class="fa-icon fa-icon-top-111 fa-icon-bottom-114"></i>
                            </a>
                        </h5>
                        <div id="footer-article-Galagala" style="display:none;">
                            @Html.Action("Index", "Article", new { lang = ViewBag.currentLanguage })
                        </div>
                        <p>GOGALA Co.,Ltd</p>
                        <p>87 Hoang Van Thai, District 7, Ho Chi Minh City, Viet Nam</p>
                    </div>
                </div>
                <div class="footer-copyright">
                    <div class="container">© @DateTime.Now.Year Copyright<a class="right user-color-white">Galagala Inc.</a></div>
                </div>
            </footer>
            <a class="over" draggable="true" ondrag="over_drag(event)"></a>
        </div>

        <div id="modal1" class="modal bottom-sheet">
            <div class="modal-content user-padding-5">
                <h4 style="color: black; border-bottom: 1px solid yellow;">Warning</h4>
                <p class="user-margin-0">We're updating data for a few minutes.</p>
            </div>
            <div class="modal-footer">
                <a href="#!" class="modal-action modal-close waves-effect waves-green btn-flat" style="background: red;color: white;">Close</a>
            </div>
        </div>
        <div class="user-overlay-main">
            <ul id="product_popular">
                <li class="header">@((string)ViewBag.multiRes.GetString("product", ViewBag.CultureInfo)) </li>
            </ul>
        </div>
        <div id="full">
            <span id="btn-normal-screen" data-target="show-full-screen">
                <img src="~/Images/min.png" width="40" height="40" />
            </span>
            <div id="contain"></div>
        </div>
        <iframe style="display:none" height="0" width="0" id="loader"></iframe>
    </div>
    <script>
        var galagala_config = {
            HOST: "http://galagala.vn:88",
            HOST_MEDIA: "http://galagala.vn:8888",
            lang: {
                next: '@((string)ViewBag.multiRes.GetString("next", ViewBag.CultureInfo))',
                prev: '@((string)ViewBag.multiRes.GetString("prev", ViewBag.CultureInfo))'
            }
        }
        var dataLink = {
            //m : mall, s : store, p: Product, c: Category, s:Search, b: Brand.
            m: '@Url.Action("","")',
            s: {
                all: {
                    link: '@Url.Action("s_all", "Particle")',
                    target: '#h_store_all'
                },
                filter: {
                    link: '@Url.Action("GetStore", "Particle")',
                    target: '#h_store_all'
                }
            },
            p: {
                buy: {
                    link: '@Url.Action("p", "Particle", new { type=1})',
                    target: '#h_product_buy',
                    next: '.swiper-button-next-swiper4',
                    prev: '.swiper-button-prev-swiper4'
                },
                hot: {
                    link: '@Url.Action("p", "Particle", new { type=3})',
                    target: '#h_product_hot',
                    next: '.swiper-button-next-swiper5',
                    prev: '.swiper-button-prev-swiper5'
                },
                recent: {
                    link: '@Url.Action("p", "Particle", new { type=4})',
                    target: '#h_product_recent',
                    next: '.swiper-button-next-swiper-recent',
                    prev: '.swiper-button-prev-swiper-recent'
                }
            },
            b: {
                top: {
                    link: '@Url.Action("b_top", "Particle")',
                    target: '#h_brand_all'
                }
            },
            c: {
                index: {
                    link: '@Url.Action("c_index", "Particle", new { lang = ViewBag.currentLanguage })',
                    target: '#content-Category',
                    Title: '@((string)ViewBag.multiRes.GetString("categories", ViewBag.CultureInfo))'
                }
            }

        }
        function over_drag(event) {
            $('#main-container-user').toggleClass('move-right');
        }
        function openApp() {
            var isiOS = navigator.userAgent.match('iPhone') || navigator.userAgent.match('iPod');
            var isAndroid = navigator.userAgent.match('Android');
            if (isiOS) {
                document.getElementById('loader').src = "itms-apps://itunes.apple.com/app/my-app/idxxxxxxxx?mt=999734614";
            }
            else
                if (1 == 1) {
                    var a_i = g_c('a_i'), a_c = g_c('a_c');
                    if (a_i == "" && a_c == "") {
                        var smlink = '<div id="smartbanner" class="ios" style=" position: fixed; bottom: 0px; z-index: 99; height: 79px;">'
                            + '<div class="sb-container">'
                            + '   <a href="#" class="sb-close" id="btn-Clost-smartBanner">×</a>'
                            + '   <span class="sb-icon gloss" style="background-image: url(http://galagala.vn:88/Content/icon.png);"></span>'
                            + '   <div class="sb-info">'
                            + '       <strong>GALAGALA - website bán hàng trực tuyến</strong>'
                            + '       <span>IOS - ANDROID</span>'
                            + '       <span>GALAGALA.VN</span>'
                            + '   </div>'
                            + '   <a href="#" onclick="alert(\'Update later.\')" class="sb-button">'
                            + '       <span>VIEW</span>'
                            + '   </a>'
                            + ' </div>'
                            + ' </div>';
                        $("#main-body-content").append(smlink);
                    }
                }
        }
        (function () {
            var fallbackLink = 'http://galagala.vn:88/' + window.location.search + window.location.hash;
            var isiOS = navigator.userAgent.match('iPad') || navigator.userAgent.match('iPhone') || navigator.userAgent.match('iPod'),
                isAndroid = navigator.userAgent.match('Android');
            if (isiOS || isAndroid) {
                document.getElementById('loader').src = 'custom-protocol://galagala' + window.location.search + window.location.hash;
                fallbackLink = isAndroid ? 'https://play.google.com/store/apps/details?id=com.hopthanh.gala.app' :
                                           'itms-apps://itunes.apple.com/app/my-app/idxxxxxxxx?mt=999734614';
            }
        })();
    </script>
    @Scripts.Render("~/www.galagala.vn/jquery")
    @Scripts.Render("~/www.galagala.vn/jqueryajax")
    <script>
        ; var gMessage = [],
            gLogined = false,
            gLang = '@ViewBag.currentLanguage', g_Category = { name_1: '@Html.Raw(ViewBag.gCateName_1)', lv: '@ViewBag.gCateLv', cate_1: '@ViewBag.gCateId_1', name_2: '@Html.Raw(ViewBag.gCateName_2)', cate_2: '@ViewBag.gCateId_2' };
        (function ($) {
            if (g_Category.lv != "") {
                if (g_Category.lv == "0") {
                    $("#lstCategory1").show()
                    $("#side-nav-left-content").animate({ 'margin-left': '-280px' });
                }
                if (g_Category.lv == "1") {
                    $("#lstCategory2").show()
                    $("#side-nav-left-content").animate({ 'margin-left': '-600px' });
                }
                if (g_Category.lv == "2") {
                    $("#lstCategory3").show()
                    
                    $("#side-nav-left-content").animate({ 'margin-left': '-900px' });
                    
                }
            }
            @if (Session["sessionGala"] != null)
        {
            <text>gLogined = true;</text>
        }
            $(document).ready(function () {
                var o_d = document.getElementsByClassName("over")[0];
                o_d.addEventListener("touchstart", over_drag, false);
                @if (ViewBag.gAlert)
                {
                    <text>$("#modal1").openModal()</text>
                }
                $(".accordion1").rjAccordion();
            })
            @if (TempData["AlertMessage"]!=null)
        {
            <text>alert('@TempData["AlertMessage"]');</text>
        }
            $(".over, #menu-btnMenuLeftNew").click(function () {
                $("#main-container-user").toggleClass("move-right");
                if ($("#main-container-user").hasClass("move-right")) {
                    $(document).scrollTop(0)
                }
            })
        })(jQuery);
    </script>
    @Scripts.Render("~/www.galagala.vn/vs-mb")
    @RenderSection("JavaScript", required: false)
    @{
        TempData.Remove("gMessageCurrent");
    }
