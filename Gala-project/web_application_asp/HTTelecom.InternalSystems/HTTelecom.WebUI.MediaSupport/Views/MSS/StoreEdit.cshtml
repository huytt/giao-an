                                                            {
                                                            <td><span class="label label-success" data-toggle="tooltip" data-placement="top" data-original-title="Active">Active</span></td>

                                                            }
                                                            else
                                                            {
                                                            <td><span class="label label-danger" data-toggle="tooltip" data-placement="top" data-original-title="Inactive">Inactive</span></td>
                                                            }
                                                        <td>@itemVendor.VendorFullName</td>
                                                        <td>@item.Address</td>
                                                        <td>
                                                            <button type="button" class="btn btn-grey mb5" onclick="VendorAddressDetail(@item.VendorAddressId)">Detail</button>
                                                        </td>
                                                    </tr>
                                                            i++;
                                                    }
                                        </tbody>
                                    </table>
                                </div>
                            </div>

