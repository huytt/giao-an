    <div class="user-position-relative" id="main-body-content" style="margin: 0 auto; display:none;">
            <header class="navbar-fixed" id="header">
                <nav id="navTop" class="backgournd-color-main" data-element="background" style=" -webkit-backface-visibility: hidden; top: 0px; padding-top: 0px; left: 0; right: 0; width: 100%; position: fixed;">
                    <div class="nav-wrapper backgournd-color-main" id="header-menu" data-element="background">
                        <ul class="right user-padding-right-10" id="user-header-content">
                            <li class="user-reponsive-relative">
                                <a href="@Url.Action("Wishlist","product")">
                                    <i class="fa-icon fa-icon-heart-23" style="  font-size: 22px;"></i>
                                    @Html.Raw("<span class='user-header-count' id='header-count-wishlist'>" + ViewBag.Layout_CountWishList + "</span>")
                                </a>
                            </li>
                            <li class="user-reponsive-relative">
                                <a href="@Url.Action("Cart", "Product" )">
                                    <i class="fa-icon fa-icon-cart-21"></i>
                                    @Html.Raw("<span class='user-header-count' id='header-count-cart'>" + ViewBag.Layout_CountCart + "</span>")
                                </a>
                            </li>
                        </ul>
                        <ul class="left" id="user-header-menu">
                            <li id="btnbacktoback"><a href="@ViewBag.Back"><i class="fa-icon fa-icon-left-82"></i></a></li>
                            <li><a href="#" id="menu-btnMenuLeft" data-activates="side-nav-left" class="button-collapse"><i class="fa-icon fa-icon-menu-66"></i></a></li>
                            <li><a id="header-btnSearch" class="cursor-point"><i class="fa-icon fa-icon-search-52"></i></a></li>
                        </ul>
                        <a href="/">
                            <img src="/Images/galagala_wed.png" style="position: absolute;max-width: 100%;max-height: 100%;margin: 0 auto;left: 50%;height: 42px;padding: 8px;margin-left: -46.5px;">
                        </a>
                    </div>
                    <ul id="side-nav-left" class="side-nav" style="height:100%;">
                        <li style="height:42px;">
                        </li>
                        <li style=" line-height: 2;">
                            <label class="change_language">
                                @((string)ViewBag.multiRes.GetString("Langague", ViewBag.CultureInfo))
                            </label>
                            @{
                                var nameCheck = "";
                                var nameUnCheck = "";
                                var nameCheck_1 = "";
                                if (Session["languageKey"] == null || Session["languageKey"] == "vi")
                                {
                                    nameUnCheck = "Vietnamese";
                                    nameCheck = "English";
                                    nameCheck_1 = "Chinese";
                                }
                                else if (Session["languageKey"] == "en")
                                {
                                    nameUnCheck = "English";
                                    nameCheck = "Vietnamese";
                                    nameCheck_1 = "Chinese";
                                }
                                else
                                {
                                    nameUnCheck = "Chinese";
                                    nameCheck = "Vietnamese";
                                    nameCheck_1 = "English";
                                }
                            }
                            <select class="browser-default" id="selLanguage" style="color:black;" data-url="@Url.Action("setLanguage", "Home", new { lang = "_lang_",u= ViewBag.u})" onchange='changeLanguage(this)' data-mini="true">
                                <option value="@nameUnCheck" disabled selected> @nameUnCheck</option>
                                <option value="@nameCheck"> @nameCheck</option>
                                <option value="@nameCheck_1">
                                    @nameCheck_1
                                </option>
                            </select>
                        </li>
                        @Html.Action("Index", "Customer")
                        @Html.Action("Index", "Category", new { lang = ViewBag.currentLanguage })
                    </ul>
                </nav>
                <nav id="navTop-Search" class="backgournd-color-main" data-element="background" style="-webkit-backface-visibility: hidden;top: 0px;padding-top: 0px;left: 0;right: 0;width: 100%;position: fixed;">
                    <div class="nav-wrapper">
                        <div class="input-field" style=" display: block; margin: 0; height: 42px; line-height: 42px;">
                            <div style="width: 45px;height: 100%;float: left;text-align: center;padding:5px;" id="btn-close-Search">
                                <span style="" class="fa-icon fa-icon-left-82">
                                </span>
                            </div>
                            <div style="margin: 0 0 0 42px;height: 42px;background-color: 0px 42px;padding: 3px;">
                                <input id="Seach-txtSearch" name="Seach-txtSearch" type="search" placeholder="@((string)ViewBag.multiRes.GetString("input_product", ViewBag.CultureInfo))" required="" style="float: left; margin: 0px; padding: 0px; line-height: 32px; background-color: white; width: 100%; font-size: 14px;">
                                <i class="mdi-navigation-close fa-icon fa-icon-search-51 active" id="Seach-btnSearch" data-href="@Url.Action("Index", "Search" , new {q="__q__" })"></i>
                            </div>
                        </div>
                    </div>
                </nav>
