                                    <a style="float: left; padding: 0px 10px; width: 100%; " data-show="3" class="mLeft-move cursor-point" data-value="-600px" data-target="#side-nav-left-content">
                                        <i class="fa-icon fa-icon-left-81 user-transform-035" style="float: left; margin-top: 6px; "></i>
                                        <span class="title-category" data-catelv="3">@ViewBag.gCateName_2</span>
                                    </a>
                                </li>
                                @foreach (var item in lv3)
                                {
                                    if (item.Logo_Url != null)
                                    {
                                        var mClass = "";
                                <li class="mLeft-item">
                                    <a class="children-after @mClass" href="@item.Link" data-catelv="3" style="min-height: 0px;" data-target="#side-nav-left-content" data-value="-900px" data-id="@item.CategoryId"
                                       data-parent="@item.ParentCateId">
                                        <p style=" height: 44px; overflow: hidden; margin: 0px;">@item.CategoryName</p>
                                        <div style="position: absolute;top: 0px;right: 0px;background-color: white;margin-right: 5px;">
                                        </div>
                                    </a>
                                </li>
                                    }
                                }
                            </ul>
                        </div>
                    </div>
                </div>
